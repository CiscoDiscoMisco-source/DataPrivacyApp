# Flask Backend Configuration
# Copy this file to .env and fill in the values for your environment

# API Configuration
REACT_APP_API_URL=http://localhost:5001

# Auth Configuration
# OAuth settings for Google login
REACT_APP_OAUTH_REDIRECT_URI=http://localhost:3000/oauth/callback

# Storage Configuration
REACT_APP_S3_REGION=us-east-1
REACT_APP_S3_BUCKET=your-bucket-name

# Environment
REACT_APP_ENV=development

# React application configuration
REACT_APP_API_BASE_URL=/api

# Backend API configuration
FLASK_APP=backend/app.py
FLASK_ENV=development
FLASK_DEBUG=1
SECRET_KEY=your-secret-key-here

# Database configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=data_privacy
DB_USER=postgres
DB_PASSWORD=your-password-here

# Elasticsearch configuration
ES_HOST=https://your-elasticsearch-instance.elastic.co
ES_PORT=9243
ES_USERNAME=elastic
ES_PASSWORD=your-elasticsearch-password-here

# AWS configuration
AWS_REGION=us-east-1
AWS_PROFILE=default 